<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Theme.SethPOS" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/flutter_blue_500</item>
        <item name="colorPrimaryVariant">@color/flutter_blue_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/flutter_blue_500</item> <!-- Using primary blue for secondary -->
        <item name="colorSecondaryVariant">@color/flutter_blue_700</item>
        <item name="colorOnSecondary">@color/white</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->

        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="homeAsUpIndicator">@drawable/ic_arrow_back</item>

        <item name="android:windowBackground">@color/white</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowDisablePreview">true</item>
        <item name="android:hardwareAccelerated">true</item>
    </style>
</resources>